{
    "name": "Nepali DateTime",
    "description": "convert A.D to B.S and B.S to A.D",
    "version": "1.0",
    "manifest_version":3,
    "permissions": [
        "contextMenus",
        "scripting",
        "activeTab",
        "storage"
    ],
    "icons": {
        "16": "./icons/img16.png",
        "48": "./icons/img48.png",
        "128": "./icons/img128.png"
    },
    "background": {
        "service_worker":"background.js"
    },
    "action": {
        "default_popup": "/popup/popup.html"
    }
}